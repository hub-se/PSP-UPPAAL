<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC "-//Uppaal Team//DTD Flat System 1.1//EN" "flat-1_1.dtd">
<nta>
	<declaration>// Place global declarations here.
broadcast chan Q_reached, S_reached, P_left;
int t1, t2;</declaration>
	<template>
		<name x="5" y="5">Until_State_After_Timed</name>
		<declaration>// Place local declarations here.
clock c;</declaration>
		<location id="id0" x="-1232" y="-365">
		</location>
		<location id="id1" x="-986" y="-306">
			<name x="-996" y="-340">O1</name>
		</location>
		<location id="id2" x="-986" y="-365">
			<name x="-996" y="-399">ERROR</name>
		</location>
		<location id="id3" x="-1113" y="-365">
			<name x="-1123" y="-399">INIT</name>
		</location>
		<init ref="id0"/>
		<transition>
			<source ref="id0"/>
			<target ref="id3"/>
			<label kind="synchronisation" x="-1215" y="-382">Q_reached?</label>
			<label kind="assignment" x="-1214" y="-365">c = 0</label>
		</transition>
		<transition>
			<source ref="id3"/>
			<target ref="id1"/>
			<label kind="guard" x="-1096" y="-306">c &lt;= t2 &amp;&amp;
c &gt;= t1</label>
			<label kind="synchronisation" x="-1095" y="-323">S_reached?</label>
			<nail x="-1113" y="-306"/>
		</transition>
		<transition>
			<source ref="id3"/>
			<target ref="id2"/>
			<label kind="synchronisation" x="-1088" y="-382">P_left?</label>
		</transition>
	</template>
	<system>// Place template instantiations here.
Observer = Until_State_After_Timed();
// List one or more processes to be composed into a system.
system Observer;
    </system>
	<queries>
		<query>
			<formula>A[] Observer.INIT imply $P
			</formula>
			<comment>
			</comment>
		</query>
		<query>
			<formula>Observer.INIT --&gt; Observer.O1
			</formula>
			<comment>
			</comment>
		</query>
	</queries>
</nta>
